<!DOCTYPE html>
<html lang="en">
<head>
    <title>32 Lesson</title>
    <link rel="stylesheet" href="index.css">
</head>
<body>
    <script>
   let пользователь = {
    имя: "John",   // свойство "имя" с значением "John"
    возраст: 30,    // свойство "возраст" с значением 30
    "любимый цвет": "синий"  // свойство с именем, содержащим пробел, нужно заключать в кавычки
};
console.log(пользователь.имя);  // выводит "John"
console.log(пользователь["возраст"]);  // также выводит 30
console.log(пользователь["любимый цвет"]);  // выводит "синий"

    </script>
</body>
</html>
